            TwoMeteorBonus, [+10% Explosion Resistance] [-5% Movement Speed], 1, FullMeteor.2
            ThreeMeteorBonus, [+10% Explosion Resistance], 1, FullMeteor.3
            FullMeteorBonus, [+10% Explosion Resistance] [Fireskin], 1, FullMeteor.4
            
            OneGolemBonus, na, 2, FullGolem.1
            TwoGolemBonus, [-5% Attack Damage], 1, FullGolem.2
            ThreeGolemBonus, na, 2, FullGolem.3
            FullGolemBonus, [Diamondskin] [-10% Attack Damage], 1, FullGolem.4
            
