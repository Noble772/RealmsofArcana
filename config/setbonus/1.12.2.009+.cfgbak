        # FILLSCREEN Attribute modifiers you receive as part of a bonus
        # 
        # Layout is...
        # Bonus id, attribute = amount @ operation, attribute = amount @ operation, etc.
        # 
        # eg...
        # 
        # This makes the Dual Wield bonus from the bonus example give a 50% attack damage bonus
        # DualWieldSnD, generic.attackDamage = 0.5 @ 1
        #  
        S:"3. Attribute Modifiers" <
            FullGravititeBonus, potioncore.stepHeight = 0.5 @ 0, extraalchemy:effect.concentration, extraalchemy:concentration_normal
            FullPhoenixBonus, generic.attackDamage = 0.15 @ 1, generic.maxHealth = -0.2 @ 1
            FullPhoenixBonus, potioncore:revival.4.1200.1200
            FullNeptuneBonus, wards:effect_aqua_affinity.1, wards:effect_depth_strider.3, wards:effect_respiration.3
            
            
            FullLeatherArmorBonus, generic.movementSpeed = 0.05 @ 1
            
            OneGolemBonus, generic.movementSpeed = -0.05 @ 1
            TwoGolemBonus, generic.movementSpeed = -0.05 @ 1, generic.attackDamage = -0.05 @ 1
            ThreeGolemBonus, generic.movementSpeed = -0.05 @ 1
            FullGolemBonus, generic.movementSpeed = -0.05 @ 1, generic.attackDamage = -0.10 @ 1
            
            TwoMeteorBonus, generic.movementSpeed = -0.05 @ 1
            
         >

        # FILLSCREEN Constant potion effects received as part of a bonus
        # 
        # Layout is...
        # Bonus id, potion.level.duration.interval, potion.level.duration.interval, etc.
        # 
        # eg...
        # 
        # This makes the Dual Wield bonus from the bonus example give haste 2
        # DualWieldSnD, haste.2
        # 
        # And this does the same thing, but the potion effect is only active for 5 seconds, every 10 seconds (5 on, 5 off, repeat)
        # DualWieldSnD, haste.2.100.200
        #  
        S:"4. Potion Effects" <
            
            FullGravititeBonus, minecraft:jump_boost.4, potioncore:slow_fall.1
            FullValkyrieBonus, wards:effect_feather_falling.1, potioncore:reach.1, potioncore:purity.1
            FullZaniteBonus, haste.2
            FullPhoenixBonus, potioncore:revival.4.1200.1200
            FullNeptuneBonus, wards:effect_aqua_affinity.1, wards:effect_depth_strider.3, wards:effect_respiration.3
            
            FullVulcaniteBonus, ebwizardry:fireskin.1, simpledifficulty:heat_resist.1
            
            FullLeatherArmorBonus, elenaidodge2:long_replenishment
            
            FullGoldArmorBonus, regeneration, extraalchemy:concentration_normal
            
            FullMeteorBonus, ebwizardry:fireskin.1
            
            FullGolemBonus, potioncore:diamond_skin.1
            
            FullYetiBonus, ebwizardry:ice_shroud.1, simpledifficulty:cold_resist.1
            
